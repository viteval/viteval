{
  "name": "@viteval/core",
  "version": "0.1.3",
  "description": "A LLM evaluation framework for JavaScript and TypeScript built on top of Vitest",
  "type": "module",
  "main": "dist/main/index.js",
  "types": "dist/main/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/zrosenbauer/viteval"
  },
  "bugs": {
    "url": "https://github.com/zrosenbauer/viteval/issues"
  },
  "homepage": "https://github.com/zrosenbauer/viteval#readme",
  "exports": {
    ".": {
      "import": "./dist/main/index.js",
      "types": "./dist/main/index.d.ts"
    },
    "./config": {
      "import": "./dist/config/index.js",
      "types": "./dist/config/index.d.ts"
    },
    "./dataset": {
      "import": "./dist/dataset/index.js",
      "types": "./dist/dataset/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "types": "tsc --noEmit",
    "test": "vitest --typecheck --passWithNoTests",
    "check": "biome check . && biome format --check .",
    "fix": "biome check --write . && biome format --write .",
    "validate": "pnpm check && pnpm test && pnpm build"
  },
  "dependencies": {
    "@vitest/runner": "^3.2.4",
    "@viteval/internal": "workspace:^",
    "ajv": "^8.0.0",
    "autoevals": "^0.0.130",
    "chalk": "^5.5.0",
    "compute-cosine-similarity": "^1.1.0",
    "find-up": "^7.0.0",
    "js-levenshtein": "^1.1.6",
    "js-yaml": "^4.1.0",
    "linear-sum-assignment": "^1.0.0",
    "mustache": "^4.2.0",
    "openai": "^4.104.0",
    "ts-pattern": "^5.8.0",
    "vite-node": "^3.2.4",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.4",
    "@tsconfig/node18": "^18.0.0",
    "@types/js-levenshtein": "^1.1.3",
    "@types/js-yaml": "^4.0.9",
    "@types/mustache": "^4.2.6",
    "@types/node": "^24.2.1",
    "dotenv": "^17.2.1",
    "tsup": "^8.0.0",
    "type-fest": "^4.41.0",
    "typescript": "^5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
