{
  "name": "viteval",
  "description": "Next generation LLM evaluation framework powered by Vitest.",
  "version": "0.5.8",
  "author": "viteval",
  "bin": {
    "viteval": "./cli.mjs"
  },
  "bugs": "https://github.com/viteval/viteval/issues",
  "dependencies": {
    "@viteval/cli": "workspace:*",
    "@viteval/core": "workspace:*",
    "@viteval/internal": "workspace:*",
    "@viteval/ui": "workspace:*",
    "vitest": "^4.0.15"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@tsconfig/node18": "^18.2.6",
    "@types/node": "^24.10.1",
    "tsdown": "^0.19.0-beta.3",
    "typescript": "^5.9.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.js"
    },
    "./dataset": {
      "types": "./dist/dataset.d.ts",
      "import": "./dist/dataset.js"
    }
  },
  "homepage": "https://github.com/viteval/viteval#readme",
  "keywords": [
    "ai",
    "evaluation",
    "javascript",
    "llm",
    "testing",
    "typescript",
    "vitest"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "viteval/viteval",
  "scripts": {
    "build": "tsdown",
    "check": "biome check .",
    "dev": "tsdown --watch",
    "fix": "biome check --write .",
    "test": "vitest --typecheck --passWithNoTests",
    "types": "tsc --noEmit"
  },
  "type": "module",
  "types": "dist/index.d.ts"
}
