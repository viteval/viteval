{
  "name": "@viteval/cli",
  "version": "0.1.4",
  "description": "Command line interface for viteval - LLM evaluation framework",
  "type": "module",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/zrosenbauer/viteval"
  },
  "bugs": {
    "url": "https://github.com/zrosenbauer/viteval/issues"
  },
  "homepage": "https://github.com/zrosenbauer/viteval#readme",
  "bin": {
    "viteval": "dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "types": "tsc --noEmit",
    "test": "vitest --typecheck --passWithNoTests",
    "check": "biome check . && biome format --check .",
    "fix": "biome check --write .",
    "validate": "pnpm check && pnpm test && pnpm build"
  },
  "dependencies": {
    "@viteval/core": "workspace:*",
    "@viteval/internal": "workspace:^",
    "consola": "^3.4.2",
    "find-up": "^7.0.0",
    "glob": "^11.0.3",
    "jiti": "2.5.1",
    "ts-pattern": "^5.8.0",
    "type-fest": "^4.41.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.4",
    "@tsconfig/node18": "^18.2.4",
    "@types/node": "^24.2.1",
    "@types/yargs": "^17.0.33",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
