{
  "name": "ui",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/ui/src",
  "targets": {
    "build": {
      "command": "echo \"Building UI\"",
      "dependsOn": ["build:server"]
    },
    "build:server": {
      "dependsOn": ["build:ui"]
    },
    "types": {
      "command": "echo \"Checking types\"",
      "dependsOn": ["build:server"]
    },
    "validate": {
      "command": "echo \"Running validation\"",
      "dependsOn": ["test", "types", "check"]
    }
  }
}
