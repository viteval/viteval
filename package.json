{
  "name": "viteval-workspace",
  "version": "1.0.0",
  "description": "Next generation LLM evaluation framework powered by Vitest.",
  "private": true,
  "scripts": {
    "build": "nx run-many -t build --parallel --projects=packages/*",
    "build:docs": "nx run-many -t build --parallel --p docs",
    "dev": "nx run-many -t dev --parallel --projects=packages/*",
    "dev:docs": "nx dev docs",
    "test": "nx run-many -t test --parallel --projects=packages/*",
    "types": "nx run-many -t types --parallel --projects=packages/*",
    "check": "nx run-many -t check",
    "fix": "nx run-many -t fix",
    "nuke": "nuka-code",
    "gen": "nx generate",
    "publish": "pnpm run build && pnpm changeset publish",
    "version": "pnpm changeset version && pnpm install --no-frozen-lockfile",
    "agents": "CI=true nx run-many --tui=false --parallel --projects=packages/* -t"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@changesets/cli": "^2.29.6",
    "@nx/js": "21.4.1",
    "@nx/plugin": "21.4.1",
    "@nx/vite": "21.4.1",
    "@nx/web": "21.4.1",
    "@nx/workspace": "^21.4.1",
    "@swc-node/register": "~1.11.1",
    "@swc/cli": "~0.7.8",
    "@swc/core": "~1.13.5",
    "@swc/helpers": "~0.5.11",
    "@tsconfig/node24": "^24.0.1",
    "@types/node": "24.3.1",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.0",
    "jiti": "2.5.1",
    "jsdom": "~26.1.0",
    "nuka-code": "^0.1.12",
    "nx": "21.4.1",
    "prettier": "^3.6.2",
    "tslib": "^2.3.0",
    "tsup": "^8.5.0",
    "typescript": "~5.9.2",
    "vite": "^7.1.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "packageManager": "pnpm@10.14.0"
}
